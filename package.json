{
  "name": "3swapdex",
  "version": "1.0.0",
  "description": "3swap official website",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/server.js",
    "server:dev": "nodemon backend/server.ts",
    "client": "sudo npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build:server": "tsc",
    "build:frontend": "cd frontend && sudo yarn build"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.6.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.18",
    "concurrently": "^7.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3Swap/official_website_updated.git"
  },
